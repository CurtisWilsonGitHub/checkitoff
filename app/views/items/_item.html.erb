<h2> <%= current_user.name %> To Do Items </h3>
<div class = "row">
    <div class = "col-md-8">
        <% @items.each do |item| %>
          <div class="media">
            <div class="media-body">
                <h4 class="media-heading">
                    <%= item.name %>
                </h4>
            </div>
          </div>
        <% end %>
</div>
